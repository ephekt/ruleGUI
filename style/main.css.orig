body {
	  background-color: #F8F8F8;
	}

input.rounded {
	border: 1px solid #ccc;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 10px;
	-moz-box-shadow: 2px 2px 3px #666;
	-webkit-box-shadow: 2px 2px 3px #666;
	box-shadow: 2px 2px 3px #666;
	font-size: 14px;
	padding: 4px 7px;
	outline: 0;
	-webkit-appearance: none;
}
input.rounded:focus {
	border-color: #9999FF;
}

.draggable{
    border: 1px solid #ccc;
    height:2em;
    width:4em;
    line-height:2em;
    padding: 0.2em;
    margin-left: 0.2em;
    margin-right: 0.2em;
    display:inline-block;
    border-radius:20px;
    -moz-border-radius:20px;
    -webkit-border-radius:20px;
    -khtml-border-radius:20px;
    font-size:150%;
    color:#000;
    text-align:center;
    background:#fff}

.draggable:hover{
color:#000;
text-decoration:none;
background:#bbb}

.draggable:disabled{
    background:#aaa;}



.circle{
width:120px;
display:table-cell;
border-radius:20px;
-moz-border-radius:20px;
-webkit-border-radius:20px;
-khtml-border-radius:20px;
font-size:15px;
color:#000;
vertical-align:middle;
text-align:center;
background:#fff}

