#dropdowns {
  display: table;
  height: 42px;
  line-height: 42px;
  color: #fff;
}
#dropdowns .dropdown {
  display: inline-block;
  position: relative;
  z-index: 4;
  border-right: 1px solid #606060;
  cursor: pointer;
}
#dropdowns .dropdown .text {
  font-family: 'ProximaNova-Bold';
  line-height: 42px;
  height: 42px;
  display: inline-block;
  padding-left: 15px;
}
#dropdowns .dropdown.focused {
  background-color: #4c4d4e;
}
#dropdowns .dropdown .drop_button {
  display: inline-block;
  cursor: pointer;
  display: block;
  display: inline-block;
  background-image: url('/Resources/core/img/arrow-large-icons.png');
  width: 40px;
  height: 40px;
  background-position: 0px 0px;
  content: '';
  display: block;
  display: inline-block;
  height: 40px;
  width: 40px;
  vertical-align: middle;
}
